set smtp_pass = "$CROSSWORD_SENDER_EMAIL_APP_PASSWORD"
set from = "$CROSSWORD_SENDER_EMAIL_ADDRESS_PREFIX@$CROSSWORD_SENDER_EMAIL_ADDRESS_DOMAIN"
